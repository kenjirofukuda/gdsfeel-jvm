<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="5.0" vgap="5.0" xmlns:fx="http://javafx.com/fxml" fx:controller="com.gdsfeel.fx.ui.GdsItemPaneController">
  <children>
    <Label text="angle:" GridPane.columnIndex="0" GridPane.rowIndex="0">
      <labelFor>
        <TextField fx:id="angleTF" GridPane.columnIndex="1" GridPane.rowIndex="0" />
      </labelFor>
    </Label>
    <fx:reference source="angleTF" />
    <Label text="mag:" GridPane.columnIndex="0" GridPane.rowIndex="1">
      <labelFor>
        <TextField fx:id="magTF" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      </labelFor>
    </Label>
    <fx:reference source="magTF" />
    <Label text="reflected:" GridPane.columnIndex="0" GridPane.rowIndex="2">
      <labelFor>
        <CheckBox  fx:id="reflectedCB" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      </labelFor>
    </Label>
    <fx:reference source="reflectedCB" />
  </children>
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
  <!--
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
  -->
  <stylesheets>
    <URL value="@gdsitempane.css" />
  </stylesheets>
</GridPane>
